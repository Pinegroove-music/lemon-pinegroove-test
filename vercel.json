{
  "rewrites": [
    {
      "source": "/(track|album)/:slug",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|WhatsApp|Twitterbot|LinkedInBot|TelegramBot|Slackbot|googlebot|bingbot).*"
        }
      ],
      "destination": "/api/seo-handler?type=$1&slug=:slug"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}